#######################################################Ffuf:
ffuf -u <target_url>/FUZZ -w <wordlist_file> -e .<extension>

####################################################### Gobuster
Easy search
gobuster dir -u "url" -w "diccionario"

add files with extensions

gobuster dir -u <target_url> -w <wordlist_file> -x <extension>


###################################################### wpscan
wpscan -url "" 
wpscan -url "" --usernames "" --passwords "dictio"



